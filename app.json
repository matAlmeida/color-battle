{
  "expo": {
    "name": "color-battle",
    "description": "An React Native application of the game Color Battle",
    "slug": "color-battle",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matalmeida.colorbattle"
    },
    "android": {
      "package": "com.matalmeida.colorbattle",
      "versionCode": 1
    },
    "githubUrl": "https://github.com/matAlmeida/color-battle"
  }
}
